
<div class="fishes-cards">
  <div class="cards">
    <% @fish.each do |fish| %>
    <div class="fish-card">
      <img class="fish-image">
      <div class="fish-informations">
      <div class="text-card"><h2><%= fish.title %></h2>
      <p><%= fish.specie %></p>
      <p><%= fish.location %></p></div>
      <div class="rating">
      <h3>♥ 5</h3>
      <h2><%= fish.daily_price %>€</h2>
      </div>
      </div>
    </div>
    <% end %>
  </div>
</div>
